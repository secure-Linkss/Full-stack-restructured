{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/q/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/validate",
      "dest": "/api/index.py"
    },
    {
      "src": "/route",
      "dest": "/api/index.py"
    },
    {
      "src": "/t/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/p/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}